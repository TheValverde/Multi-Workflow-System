# === LLM Provider Keys ===
# OpenAI API Key (required for LangGraph agent)
OPENAI_API_KEY=sk-...

# Anthropic API Key (optional, for Claude models)
ANTHROPIC_API_KEY=sk-ant-...

# === Supabase Configuration ===
# Supabase Project URL (format: https://<project-ref>.supabase.co)
SUPABASE_URL=https://your-project.supabase.co

# Supabase Anon/Public Key
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Supabase Service Role Key (for admin operations, keep secret!)
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# === LangGraph Configuration ===
# LangGraph deployment URL (for CopilotKit integration)
LANGGRAPH_DEPLOYMENT_URL=http://localhost:8123

# LangSmith API Key (optional, for tracing and monitoring)
LANGSMITH_API_KEY=ls__...

# LangSmith Project Name (optional)
LANGSMITH_PROJECT=fs-agent

# === CopilotKit Configuration ===
# CopilotKit Public API Key (for Copilot Cloud)
NEXT_PUBLIC_CPK_PUBLIC_API_KEY=...

# === Application Configuration ===
# Next.js Port
NEXT_PUBLIC_PORT=3000

# Agent Port
AGENT_PORT=8123

# Environment (development, staging, production)
NODE_ENV=development

# === Database Configuration (if using local Postgres) ===
# POSTGRES_USER=postgres
# POSTGRES_PASSWORD=your_secure_password
# POSTGRES_DB=fs_agent
# POSTGRES_PORT=5440
